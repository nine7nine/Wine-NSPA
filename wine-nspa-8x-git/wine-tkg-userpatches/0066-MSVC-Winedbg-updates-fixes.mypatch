 /* ?_XGetLastError@std@@YAXXZ */
 void __cdecl _XGetLastError(void)
 {
-    FIXME("stub\n");
+    int err = GetLastError();
+    system_error se;
+    const char *msg;
+
+    TRACE("() GetLastError()=%d\n", err);

+            return 1;
+        strc = _mbsnextc_l(str, locale);
+        cmpc = _mbsnextc_l(cmp, locale);
+        if (strc != cmpc)
+            return strc < cmpc ? -1 : 1;
+        inc = (strc > 255) ? 2 : 1; /* Equal, use same increment */
+        str += inc;
+        cmp += inc;
     }
     return 0; /* Matched len chars */
-  }
-  return u_strncmp(str, cmp, len); /* ASCII CP */
+}

+                             (ct_ret.left && !ct_ret.right) ? " " : NULL,
+                             call_conv, call_conv ? " " : NULL, exported,
+                             name, args_str, function_qualifier, ct_ret.right);
+    ret = TRUE;
+done:
+    return ret;

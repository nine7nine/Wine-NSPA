 /* return a monotonic time counter */
 timeout_t monotonic_counter(void)
 {
@@ -534,13 +578,13 @@ timeout_t monotonic_counter(void)
 #endif
     return mach_absolute_time() * timebase.numer / timebase.denom / 100;
 #elif defined(HAVE_CLOCK_GETTIME)
-    struct timespec ts;

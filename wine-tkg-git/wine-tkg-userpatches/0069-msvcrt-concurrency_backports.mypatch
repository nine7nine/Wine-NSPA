 {
     if(timeout == COOPERATIVE_TIMEOUT_INFINITE) return NULL;
-    pTime->QuadPart = (ULONGLONG)timeout * -10000;
+    pTime->QuadPart = (ULONGLONG)timeout * -TICKSPERMSEC;
     return pTime;
 }
 
